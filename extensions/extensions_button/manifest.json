{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon-32x32.png",
    "default_title": "Extensions"
  },
  "description": "Provides quick icon access to your Chrome extensions and display how many tabs are open.",
  "icons": {
    "128": "img/icon-128x128.png",
    "16": "img/icon-16x16.png",
    "48": "img/icon-32x32.png"
  },
  "manifest_version": 2,
  "name": "Extensions Button",
  "permissions": [
    "tabs",
    "*://*/"
  ],
  "version": "0.0.1"
}