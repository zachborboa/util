{
  "applications": {
    "gecko": {
      "id": "close-tab@example.com"
    }
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "js/content_script.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "description": "Allowing closing a window's tab.",
  "manifest_version": 3,
  "name": "Close Tab",
  "permissions": [
    "tabs"
  ],
  "version": "0.0.1"
}
