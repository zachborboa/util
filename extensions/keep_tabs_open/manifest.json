{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon-48x48.png",
    "default_title": "Keep Tabs Open"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Continually check and ensure select pages are open.",
  "icons": {
    "128": "img/icon-128x128.png",
    "16": "img/icon-16x16.png",
    "48": "img/icon-48x48.png"
  },
  "manifest_version": 2,
  "name": "Keep Tabs Open",
  "options_page": "html/options.html",
  "permissions": [
    "alarms",
    "tabs",
    "storage"
  ],
  "version": "0.0.1"
}
