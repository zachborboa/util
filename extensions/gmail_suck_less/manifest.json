{
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css/hangouts_style.css"
      ],
      "matches": [
        "https://hangouts.google.com/*"
      ]
    },
    {
      "js": [
        "js/content_script.js"
      ],
      "css": [
        "css/style.css"
      ],
      "matches": [
        "https://mail.google.com/*"
      ]
    }
  ],
  "description": "Make Gmail suck less.",
  "manifest_version": 2,
  "name": "Gmail Suck Less",
  "version": "1.0.0"
}
