{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon-48x48.png",
    "default_title": "Pin Tab"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Automatically pin tabs based on url patterns and quickly pin and unpin your tabs.",
  "icons": {
    "128": "img/icon-128x128.png",
    "16": "img/icon-16x16.png",
    "48": "img/icon-48x48.png"
  },
  "manifest_version": 3,
  "name": "Pin Tab",
  "options_page": "html/options.html",
  "permissions": [
    "tabs",
    "storage"
  ],
  "version": "0.0.1"
}
